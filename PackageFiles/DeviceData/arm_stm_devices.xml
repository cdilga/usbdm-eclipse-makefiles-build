         <eraseMethodsRef ref="stmEraseMethods" />
         <memory type="flash" >
            <memoryRange start="0x00000000" size="0x1000" />
         </memory>
         <sdidAddress value="0xE0042000" />
         <sdid value="0x00000000" mask="0x00000000" />
         <note>STM 32 Default device</note>
      </device>
